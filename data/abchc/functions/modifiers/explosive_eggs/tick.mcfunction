tag @s remove abch.eeMExists
execute at @e[ type=egg ] if score @s abch.explosive_eggs.UUID1 = @e[ type=egg, limit=1, sort=nearest ] abch.explosive_eggs.UUID1 if score @s abch.explosive_eggs.UUID2 = @e[ type=egg, limit=1, sort=nearest ] abch.explosive_eggs.UUID2 if score @s abch.explosive_eggs.UUID3 = @e[ type=egg, limit=1, sort=nearest ] abch.explosive_eggs.UUID3 if score @s abch.explosive_eggs.UUID4 = @e[ type=egg, limit=1, sort=nearest ] abch.explosive_eggs.UUID4 run tp @s ~ ~ ~
execute at @e[ type=egg ] if score @s abch.explosive_eggs.UUID1 = @e[ type=egg, limit=1, sort=nearest ] abch.explosive_eggs.UUID1 if score @s abch.explosive_eggs.UUID2 = @e[ type=egg, limit=1, sort=nearest ] abch.explosive_eggs.UUID2 if score @s abch.explosive_eggs.UUID3 = @e[ type=egg, limit=1, sort=nearest ] abch.explosive_eggs.UUID3 if score @s abch.explosive_eggs.UUID4 = @e[ type=egg, limit=1, sort=nearest ] abch.explosive_eggs.UUID4 run tag @s add abch.eeMExists

execute unless entity @s[ tag=abch.eeMExists ] run summon creeper ~ ~ ~ {Fuse: 0s, ignited: 1b}
execute unless entity @s[ tag=abch.eeMExists ] run effect give @e[ type=chicken, nbt={Age: -24000} ] resistance 1 255 true
execute unless entity @s[ tag=abch.eeMExists ] run kill @s